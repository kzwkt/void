<?xml version="1.0"?>
<labwc_config>
	
<theme>
<name>xp</name>
 <icon>luna</icon>
 <cornerRadius>8</cornerRadius>
 <keepBorder>no</keepBorder>
 <dropShadows>yes</dropShadows>
</theme>

<desktops number="4" />
	<keyboard>
	<default />
	<keybind key="Super_L" onRelease="yes"><action name="Execute" command="start"/></keybind>
	<keybind key="W-e"><action name="Execute" command="foot fm"/></keybind>
	<keybind key="W-i"><action name="Execute" command="sublime_text ~/.config/labwc/rc.xml"/></keybind>
	<keybind key="W-S-i"><action name="Execute" command="sublime_text ~/.config/sfwbar/sfwbar.config"/></keybind>
	<keybind key="W-q"><action name="Close"/></keybind>
	<keybind key="W-S-r"><action name="Reconfigure"/></keybind>
	<keybind key="W-w"><action name="Execute" command="apulse ~/opt/firefox/firefox"/></keybind>
	<keybind key="W-z"><action name="Execute" command="foot"/></keybind>

	<keybind key="W-d"><action name="ForEach"><query desktop="current" /><then><action name="Iconify"/></then></action></keybind>

<keybind key="W-1">  <action name="GoToDesktop"> <to>1</to> </action>  </keybind>
<keybind key="W-2"> <action name="GoToDesktop">  <to>2</to> </action> </keybind>
<keybind key="W-3"> <action name="GoToDesktop">  <to>3</to>  </action> </keybind>
<keybind key="W-4"> <action name="GoToDesktop">  <to>4</to> </action> </keybind>
<keybind key="W-S-exclam"> <action name="SendToDesktop"> <to>1</to> </action>  </keybind>
<keybind key="W-S-at"> <action name="SendToDesktop">  <to>2</to> </action> </keybind>
<keybind key="W-S-numbersign"> <action name="SendToDesktop">  <to>3</to> </action> </keybind>
<keybind key="W-S-dollar"> <action name="SendToDesktop">  <to>4</to> </action> </keybind>
	
<keybind key="F2">  <action name="Execute" command="c.sh vup"/> </keybind>
<keybind key="F3"> <action name="Execute" command="c.sh vdown"/> </keybind>
<keybind key="F1"> <action name="Execute" command="c.sh vmute"/> </keybind>
<keybind key="F6">  <action name="Execute" command="c.sh bup"/> </keybind>
<keybind key="F7"> <action name="Execute" command="c.sh bdown"/> </keybind>
<keybind key="XF86AudioRaiseVolume">  <action name="Execute" command="c.sh vup"/> </keybind>
<keybind key="XF86AudioLowerVolume"> <action name="Execute" command="c.sh vdown"/> </keybind>
<keybind key="XF86AudioMute"> <action name="Execute" command="c.sh vmute"/> </keybind>
<keybind key="XF86MonBrightnessUp">  <action name="Execute" command="c.sh bup"/> </keybind>
<keybind key="XF86MonBrightnessDown"> <action name="Execute" command="c.sh bdown"/> </keybind>

	</keyboard>

	<mouse>
	<default />

	</mouse>
     
<windowRules>
	<windowRule title="Picture in picture">
		<action name="ToggleAlwaysOnTop" />
	</windowRule>
</windowRules>

</labwc_config>
